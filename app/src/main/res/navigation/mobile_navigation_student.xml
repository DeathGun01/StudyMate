<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_student"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.netbucket.studymate.fragments.StudentDashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_student_dashboard" />

    <fragment
        android:id="@+id/navigation_chats"
        android:name="com.netbucket.studymate.fragments.StudentChatsFragment"
        android:label="@string/title_chats"
        tools:layout="@layout/fragment_student_chats" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.netbucket.studymate.fragments.StudentNotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_student_notifications" />

</navigation>